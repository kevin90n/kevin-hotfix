{
  "tagFormat": "${version}",
  "branches": [
    "main",
    {
      "name": "hotfix",
      "prerelease": "hotfix"
    }
  ],
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    [
      "@semantic-release/github",
      {
        "successComment": false,
        "failComment": false
      }
    ],
    [
      "@saithodev/semantic-release-backmerge",
      {
        "backmergeBranches": [{"from": "main", "to": "hotfix"}]
      }
    ]
  ],
  "publish": false
}